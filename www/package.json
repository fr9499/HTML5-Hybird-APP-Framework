{
  "name": "hybird-app",
  "version": "0.0.1",
  "description": "Hybird APP Framework",
  "main": "index.js",
  "scripts": {
    "package-mac": "electron-packager . hybird-app --ignore=node_modules/electron-* --icon=img/icon.png --platform=win32 --arch=x64 --version=1.0.2",
    "package-win": "electron-packager . hybird-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=img/icon.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",    
    "package-linux": "electron-packager . hybird-app --overwrite --asar=true --platform=linux --arch=x64 --icon=img/icon.png --prune=true --out=release-builds"
    "package": "asar pack hybird-app-win32-x64/resources/app hybird-app-win32-x64/resources/app.asar && rm -rf hybird-app-win32-x64/resources/app"
  },
  "dependencies": {
    "electron-debug": "^1.1.0",
    "electron-google-oauth": "^2.0.0",
    "file-exists": "^2.0.0",
    "os-tmpdir": "^1.0.2"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron-prebuilt": "^1.4.13"
  }
}
